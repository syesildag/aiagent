{
  "servers": [
    {
      "name": "context7",
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ],
      "env": {},
      "enabled": true
    },
    {
      "name": "notes",
      "command": "node",
      "args": [
        "dist/mcp/server/notes.js"
      ],
      "enabled": true
    },
    {
      "name": "memory",
      "command": "node",
      "args": [
        "dist/mcp/server/memory.js"
      ],
      "enabled": true
    }
  ]
}