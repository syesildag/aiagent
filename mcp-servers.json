{
  "servers": [
    {
      "name": "context7",
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ],
      "env": {},
      "enabled": true
    },
    {
      "name": "memory",
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ],
      "env": {
        "MEMORY_FILE_PATH": "./memory.json"
      },
      "enabled": true
    },
    {
      "name": "notes",
      "command": "node",
      "args": [
        "dist/scripts/mcp/notes.js"
      ],
      "enabled": true
    }
  ]
}