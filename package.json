{
   "name": "node",
   "version": "1.0.0",
   "description": "NodeJS",
   "main": "dist/index.js",
   "scripts": {
      "clean": "rm -fr dist",
      "start": "NODE_ENV=production node dist/index.js",
      "dev": "NODE_ENV=development node --trace-deprecation dist/index.js",
      "compile": "tsc",
      "build": "npm run clean;npm run compile",
      "buildStart": "npm run build && npm start",
      "buildDev": "npm run build && npm run dev",
      "test": "jest",
      "dev:watch": "ts-node-dev --respawn --transpile-only src/index.ts",
      "lint": "eslint . --ext .ts",
      "lint:fix": "eslint . --ext .ts --fix"
   },
   "author": "Serkan YESILDAG",
   "license": "MIT",
   "devDependencies": {
      "@jest/globals": "^28.1.3",
      "@swc/core": "^1.7.22",
      "@types/compression": "^1.7.5",
      "@types/cookie-parser": "^1.4.7",
      "@types/cors": "^2.8.19",
      "@types/express": "^4.17.21",
      "@types/jest": "^30.0.0",
      "@types/node": "^20.16.2",
      "@types/pg": "^8.11.11",
      "@typescript-eslint/eslint-plugin": "^8.40.0",
      "@typescript-eslint/parser": "^8.40.0",
      "eslint": "^9.33.0",
      "eslint-config-prettier": "^10.1.8",
      "eslint-plugin-prettier": "^5.5.4",
      "jest": "^28.1.3",
      "opencode-ai": "^0.3.130",
      "prettier": "^3.6.2",
      "ts-jest": "^28.0.8",
      "ts-jest-resolver": "^2.0.0",
      "ts-loader": "^9.5.1",
      "ts-node": "^10.9.2",
      "ts-node-dev": "^2.0.0",
      "typescript": "^5.8.2"
   },
   "dependencies": {
      "compression": "^1.7.4",
      "cors": "^2.8.5",
      "crawlee": "^3.14.1",
      "dotenv": "^16.4.5",
      "express": "^4.19.2",
      "express-rate-limit": "^7.5.0",
      "helmet": "^8.1.0",
      "ollama": "^0.5.13",
      "pg": "^8.13.3",
      "pgvector": "^0.2.0",
      "playwright": "^1.51.1",
      "zod": "^3.24.2",
      "zod-to-json-schema": "^3.24.1"
   },
   "overrides": {
      "tmp": "^0.2.4"
   }
}
