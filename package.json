{
   "name": "node",
   "version": "1.0.0",
   "description": "NodeJS",
   "main": "dist/src/index.js",
   "scripts": {
      "clean": "rm -fr dist/src dist/examples",
      "clean:all": "rm -fr dist",
      "start": "NODE_ENV=production node dist/src/index.js",
      "dev": "NODE_ENV=development node --trace-deprecation dist/src/index.js",
      "cli": "NODE_ENV=production node dist/src/cli.js",
      "cli:dev": "NODE_ENV=development node dist/src/cli.js",
      "compile": "tsc",
      "copy:templates": "mkdir -p dist/src/frontend/templates && cp src/frontend/templates/*.html dist/src/frontend/templates/",
      "build": "npm run clean && npm run compile && npm run copy:templates",
      "build:frontend": "webpack --mode production",
      "build:all": "npm run clean:all && npm run compile && npm run copy:templates && npm run build:frontend",
      "buildStart": "npm run build && npm start",
      "buildDev": "npm run build && npm run dev",
      "buildCli": "npm run build && npm run cli",
      "buildCliDev": "npm run build && npm run cli:dev",
      "entityGen": "node dist/src/utils/entityGenerator.js",
      "buildEntityGen": "npm run build && npm run entityGen",
      "doMigrate": "node dist/src/scripts/migrate.js",
      "entrypoint": "npm run migrate && npm run addDefaultUser && npm start",
      "addDefaultUser": "npm run migrate && node dist/src/scripts/addUser.js $DEFAULT_USERNAME $DEFAULT_PASSWORD",
      "migrate": "npm run build && npm run doMigrate",
      "migrate:status": "npm run migrate -- status",
      "migrate:reset": "npm run migrate -- reset",
      "test": "jest",
      "dev:watch": "ts-node-dev --respawn --transpile-only src/index.ts",
      "lint": "eslint . --ext .ts",
      "lint:fix": "eslint . --ext .ts --fix"
   },
   "author": "Serkan YESILDAG",
   "license": "MIT",
   "devDependencies": {
      "@babel/preset-env": "^7.29.0",
      "@babel/preset-react": "^7.28.5",
      "@babel/preset-typescript": "^7.28.5",
      "@jest/globals": "^29.7.0",
      "@swc/core": "^1.7.22",
      "@types/compression": "^1.7.5",
      "@types/cookie-parser": "^1.4.7",
      "@types/cors": "^2.8.19",
      "@types/express": "^4.17.21",
      "@types/jest": "^29.5.14",
      "@types/lru-cache": "^7.10.9",
      "@types/node": "^20.16.2",
      "@types/node-schedule": "^2.1.8",
      "@types/pg": "^8.11.11",
      "@types/react": "^19.2.14",
      "@types/react-dom": "^19.2.3",
      "@typescript-eslint/eslint-plugin": "^8.40.0",
      "@typescript-eslint/parser": "^8.40.0",
      "babel-loader": "^10.0.0",
      "eslint": "^9.33.0",
      "eslint-config-prettier": "^10.1.8",
      "eslint-plugin-prettier": "^5.5.4",
      "jest": "^29.7.0",
      "opencode-ai": "^0.6.6",
      "prettier": "^3.6.2",
      "ts-jest": "^29.4.6",
      "ts-jest-resolver": "^2.0.0",
      "ts-loader": "^9.5.1",
      "ts-node": "^10.9.2",
      "ts-node-dev": "^2.0.0",
      "typescript": "^5.9.3",
      "webpack-cli": "^6.0.1"
   },
   "dependencies": {
      "@emotion/react": "^11.14.0",
      "@emotion/styled": "^11.14.1",
      "@modelcontextprotocol/sdk": "^1.18.2",
      "@mui/icons-material": "^7.3.8",
      "@mui/material": "^7.3.8",
      "@xenova/transformers": "^2.17.2",
      "compression": "^1.7.4",
      "cors": "^2.8.5",
      "crawlee": "^3.14.1",
      "dotenv": "^16.4.5",
      "express": "^4.19.2",
      "express-rate-limit": "^7.5.0",
      "helmet": "^8.1.0",
      "lru-cache": "^11.2.2",
      "node-schedule": "^2.1.1",
      "ollama": "^0.5.13",
      "pg": "^8.13.3",
      "playwright": "^1.51.1",
      "react": "^19.2.4",
      "react-dom": "^19.2.4",
      "zod": "^3.24.2",
      "zod-to-json-schema": "^3.24.1"
   },
   "overrides": {
      "tmp": "^0.2.4"
   }
}
