{
  "jsc": {
    "target": "es2022",
    "parser": {
      "syntax": "typescript",
      "tsx": true,
      "decorators": true,
      "dynamicImport": true
    },
    "transform": {
      "legacyDecorator": true,
      "decoratorMetadata": true,
      "react": {
        "runtime": "classic",
        "pragma": "React.createElement",
        "pragmaFrag": "React.Fragment"
      }
    },
    "keepClassNames": true,
    "loose": false
  },
  "module": {
    "type": "commonjs",
    "strict": false,
    "noInterop": false
  },
  "sourceMaps": true,
  "inlineSourcesContent": true
}
